.scroll_top {
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
}

.section__title {
  font-weight: 900;
  display: flex;
  flex-direction: row;
}

.section_title_right {
  display: flex;
  flex-direction: row;
}

.left_mark {
  border: 2rpx solid blue;
  margin: 10rpx 10rpx 0 0;
  height: 20rpx;
  width: 0px;
}

.right_arrow {
  width: 16rpx;
  height: 16rpx;
  border-top: 4rpx solid #000;
  border-right: 4rpx solid #000;
  transform: rotate(45deg);
  margin:12rpx 15rpx 0;
}