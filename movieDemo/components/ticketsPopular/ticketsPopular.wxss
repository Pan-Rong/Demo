
.move_list_item {
  display: flex;
  flex-direction: row;
  margin: 20rpx auto;
}

.move_list_item_text {
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-between;
  border-bottom: 1rpx solid #aaa;
  margin-left: 10rpx;
  font-size: 28rpx;
}

.move_list_item_left {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 200rpx;
}

.buy_tickets {
  margin-top: 70rpx;
  margin-right:0;
  height: 60rpx;
  width: 100rpx;
  border: 1px solid #aaa;
  background-color: white;
  border-radius: 12rpx;
  padding: 0;
  font-size: 30rpx;
  line-height: 60rpx;
}