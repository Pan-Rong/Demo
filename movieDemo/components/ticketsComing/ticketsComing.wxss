.coming_list_item {
  display: flex;
  flex-direction: row;
  margin: 20rpx auto;
}

.coming_top_title{
  width: 300rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1rpx solid #aaa;
}

.move_list_coming{
  margin: 30rpx 20rpx;
  border-bottom: 1rpx solid #aaa;
}
.coming_list_item_text{
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-between;
  margin-left: 10rpx;
  font-size: 28rpx;
}
.coming_list_item_right{
    display: flex;
    align-content: center;
}